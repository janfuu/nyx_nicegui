{
    "app": {
        "title": "Nyx",
        "host": "localhost",
        "port": 8080,
        "debug": true,
        "version": "1.0.0"
    },
    "database": {
        "path": "data/nyx_memory.db"
    },
    "llm": {
        "provider": "openrouter",
        "model": "mistralai/mistral-nemo",
        "temperature": 0.8,
        "max_tokens": 512,
        "max_new_tokens": 512,
        "truncation_length": 2048,
        "local_api_base": "http://localhost:5000/v1",
        "openrouter_api_base": "https://openrouter.ai/api/v1",
        
        "api_key": "",
        "openrouter_api_key": "",
        "http_referer": "http://localhost:8080",
        "max_messages": 20,
        "timeout": 60,
        "use_streaming": false,
        
        "parser_provider": "openrouter",
        "parser_model": "mistralai/mistral-small-24b-instruct-2501"
    },
    "image_generation": {
        "model": "civitai:443821@1478064",
        "width": 832,
        "height": 1216,
        "number_results": 1,
        "output_format": "JPEG",
        "steps": 30,
        "cfg_scale": 6,
        "scheduler": "Default",
        "output_type": ["URL"],
        "include_cost": false,
        "lora": [
            {
                "model": "civitai:1115064@1253021",
                "weight": 1
            }
        ],
        "runware_api_key": "",
        "runware_api_base": "https://api.runware.ai/v1",
        "use_prompt_enhancement": false
    }
}